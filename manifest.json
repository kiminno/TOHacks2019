{
  "manifest_version": 2,
  "name": "Bearvan",
  "description": "Place Project Description Here",
  "version": "1.0",
  "browser_action": {
    "default_icon": "ourextensionicon.png",
    "default_popup": "bearvan.html"
  },
  "permissions": [
    "activeTab",
    "geolocation",
    "webRequest"
  ],
  "content_scripts": [
    {
      "matches": [ "*://*.google.com/maps/dir/*" ],
      "js": [ "scripts.js" ]
    }
  ]
}
